<div class="c-container__carousel">

	<div class="owl-features owl-carousel">


		{% for evento in eventos %}
			<a href="{{ url({{evento.url}}) }}">
				<div class="item">
					<img src="/images/{{evento.img}}" alt="">
					<div class="down-content">
						<h4>{{evento.title}}</h4>
						<i class="fa-regular fa-eye"></i>
					</div>
				</div>
			</a>
		{% endfor %}

	</div>

</div>
